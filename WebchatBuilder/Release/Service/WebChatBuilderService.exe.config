  </configSections>
  <connectionStrings>
    <add name="Repository" connectionString="" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="CicUser" value="" />
    <add key="CicPassword" value="" />
    <add key="PrimaryServer" value="" />
    <add key="SecondaryServer" value="" />
    <add key="ServicePort" value="" />
    <add key="AvailabilityResetInterval" value="" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]\r\n" />
        <param name="Footer" value="[Footer]\r\n" />
        <param name="ConversionPattern" value="%date %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
    <appender name="MainLog" type="log4net.Appender.RollingFileAppender">
      <file value="..\logs\WCB" />
      <appendToFile value="true" />
      <datePattern value=".yyyyMMdd.lo\g" />
      <rollingStyle value="Date" />
      <maxSizeRollBackups value="7" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="MainLog" />
      <appender-ref ref="ConsoleAppender" />
    </root>
  </log4net>

